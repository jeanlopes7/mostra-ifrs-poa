<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Organizador" table="organizador">
    <field name="nivel" type="integer" column="nivel" nullable="false"/>
    <field name="status" type="integer" column="status" nullable="false"/>
    <one-to-one field="fkUsuario" target-entity="Usuario">
      <join-columns>
        <join-column name="fk_usuario" referenced-column-name="id_usuario"/>
      </join-columns>
    </one-to-one>
  </entity>
</doctrine-mapping>
